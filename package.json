{
  "name": "Simple-Static-React-Aframe",
  "version": "1.0.0",
  "description": "Minimal boilerplate and configuration for the development of static VR sites using aframe, react & es6",
  "repository": "https://github.com/Jon-Biz/simple-static-react-aframe",
  "keywords": [
    "VR",
    "AR",
    "virtual reality",
    "augmented reality",
    "boilerplate",
    "example",
    "react",
    "reactjs",
    "static",
    "es6"
  ],
  "scripts": {
    "webpack": "node ./node_modules/webpack/bin/webpack.js",
    "webpack-dev-server": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "build": "npm run webpack",
    "dev": "npm run webpack-dev-server -s -- --progress --colors --port 4000 --content-base public"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "author": "jon-biz",
  "license": "MIT",
  "dependencies": {
    "aframe": "^0.2.0",
    "aframe-react": "^1.3.0",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.23.1",
    "node-sass": "^4.5.3",
    "react-edit-inline": "^1.0.8",
    "react-transform-hmr": "^1.0.4",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.13.2",
    "webpack": "^1.15.0"
  }
}
